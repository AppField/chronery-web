{
  "name": "chronery-web",
  "version": "0.8.3",
  "license": "MIT",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "start-de": "ng serve --aot --i18nFile=src/locale/messages.de.xlf --i18nFormat=xlf --locale de --port 4300",
    "build": "ng build --prod --aot --locale=en --env=prod ",
    "build-de": "ng build --prod --aot --i18nFile=src/locale/messages.de.xlf --i18nFormat=xlf --locale=de --env=prod",
    "build-i18n": "for lang in de; do ng build --output-path=dist/$lang --aot -prod --base=href /$lang/ --i18n-file=src/locale/messages.$lang.xlf --i18n-format=xlf --locale=$lang; ng build --output-path=dist/en --aot -prod --base-href /en/ --locale=en --env=prod; done",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "deploy-ghpage": "ng build --prod --aot --env=test --base-href /chronery-web/ && angular-cli-ghpages",
    "i18n-file": "ng xi18n --outputPath src/locale",
    "pwa": "http-server -p 8080 ./dist",
    "stats": "ng build --prod --stats-json && npx webpack-bundle-analyzer dist/stats.json",
    "source-maps-explorer": "ng build --prod --sourcemaps && npx source-map-explorer dist/main.f3be1bf742c2a16b21d5.bundle.js"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^5.2.10",
    "@angular/cdk": "^5.2.5",
    "@angular/common": "^5.2.10",
    "@angular/compiler": "^5.2.10",
    "@angular/core": "^5.2.10",
    "@angular/flex-layout": "5.0.0-beta.14",
    "@angular/forms": "^5.2.10",
    "@angular/http": "^5.2.10",
    "@angular/material": "^5.2.5",
    "@angular/material-moment-adapter": "^5.2.5",
    "@angular/platform-browser": "^5.2.10",
    "@angular/platform-browser-dynamic": "^5.2.10",
    "@angular/platform-server": "^5.2.10",
    "@angular/pwa": "^0.6.1",
    "@angular/router": "^5.2.10",
    "@angular/service-worker": "^5.2.10",
    "angular2-csv": "^0.2.5",
    "aws-amplify": "^0.3.3",
    "core-js": "^2.5.5",
    "d3": "^5.1.0",
    "hammerjs": "^2.0.8",
    "jspdf": "^1.4.1",
    "jspdf-autotable": "github:simonbengtsson/jspdf-autotable#master",
    "lodash": "^4.17.10",
    "moment": "^2.22.2",
    "moment-duration-format": "^2.2.2",
    "npm": "^6.4.1",
    "rxjs": "6.3.0",
    "zone.js": "^0.8.26"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~0.7.5",
    "@angular/cli": "6.1.5",
    "@angular/compiler-cli": "^6.1.6",
    "@types/d3": "^5.0.0",
    "@types/jasmine": "2.8.8",
    "@types/jspdf": "^1.1.31",
    "@types/node": "^10.9.4",
    "angular-cli-ghpages": "^0.5.3",
    "codelyzer": "~4.4.4",
    "jasmine-core": "^3.2.1",
    "jasmine-spec-reporter": "^4.2.1",
    "karma": "~3.0.0",
    "karma-chrome-launcher": "~2.2.0",
    "karma-cli": "~1.0.1",
    "karma-coverage-istanbul-reporter": "^2.0.2",
    "karma-jasmine": "~1.1.2",
    "karma-jasmine-html-reporter": "^1.3.1",
    "protractor": "^5.4.0",
    "source-map-explorer": "^1.6.0",
    "ts-node": "^7.0.1",
    "tslint": "~5.11.0",
    "typescript": "~2.9.0",
    "webpack-bundle-analyzer": "^2.13.1"
  }
}
